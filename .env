# ==========================================
# AI 提供商配置
# ==========================================

# 默认使用的AI（推荐deepseek）
DEFAULT_AI_PROVIDER=deepseek

# DeepSeek 配置 （推荐：便宜又好用）
DEEPSEEK_API_KEY=sk-your-deepseek-key-here
DEEPSEEK_BASE_URL=https://api.deepseek.com/v1
DEEPSEEK_MODEL=deepseek-chat

# Claude 配置
CLAUDE_API_KEY=sk-ant-api03-your-claude-key-here
CLAUDE_MODEL=claude-sonnet-4-20250514

# OpenAI 配置
OPENAI_API_KEY=sk-your-openai-key-here
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_MODEL=gpt-4-turbo

# Google Gemini 配置
GEMINI_API_KEY=AIza-your-gemini-key-here

# 阿里通义千问 配置
QWEN_API_KEY=sk-your-qwen-key-here
QWEN_BASE_URL=https://dashscope.aliyuncs.com/api/v1
QWEN_MODEL=qwen-max

# 智谱GLM 配置
GLM_API_KEY=your-glm-key-here
GLM_BASE_URL=https://open.bigmodel.cn/api/paas/v4
GLM_MODEL=glm-4

# ==========================================
# 服务器配置
# ==========================================
PORT=3000
NODE_ENV=development

# ==========================================
# 测试配置
# ==========================================
DEFAULT_SAVE_PATH=./outputs/scripts
SCREENSHOT_PATH=./outputs/screenshots
REPORT_PATH=./outputs/reports